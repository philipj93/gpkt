{
  "name": "gpkt",
  "version": "0.1.8",
  "description": "Create, build, and publish new packages with the gpkt scaffolding toolkit",
  "license": "MIT",
  "author": {
    "name": "Philip Johnston",
    "url": "https://github.com/tundera",
    "email": "tunderadev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/gpkt",
    "directory": "packages/gpkt"
  },
  "keywords": [
    "gpkt",
    "package",
    "generator"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "templates",
    "README.md"
  ],
  "bin": "./bin/gpkt.js",
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "esno src/cli.ts new my-cli --project-directory ../../../example-cli --preset cli",
    "lint": "TIMING=1 eslint --ext .ts,.tsx,.js,.jsx --cache --cache-location ./node_modules/.cache/eslint .",
    "prepublishOnly": "pnpm build",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@babel/core": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "async-retry": "1.3.3",
    "colorette": "2.0.16",
    "console-clear": "1.1.1",
    "convert-hrtime": "5.0.0",
    "cpy": "^9.0.1",
    "cross-spawn": "7.0.3",
    "ejs": "3.1.7",
    "execa": "6.1.0",
    "globby": "13.1.1",
    "got": "12.0.4",
    "gradient-string": "^2.0.1",
    "jscodeshift": "0.13.1",
    "merge-anything": "^5.0.2",
    "ora": "^6.1.0",
    "prompts": "2.4.2",
    "react": "18.1.0",
    "read-pkg": "^7.1.0",
    "rimraf": "^3.0.2",
    "tar": "6.1.11",
    "unconfig": "^0.3.4",
    "update-check": "1.5.4",
    "validate-npm-package-name": "4.0.0",
    "valtio": "1.6.0",
    "yaml": "2.0.1",
    "yargs": "17.4.1"
  },
  "devDependencies": {
    "@gpkt/tsconfig": "workspace:*",
    "@types/async-retry": "1.4.4",
    "@types/console-clear": "1.1.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "3.1.0",
    "@types/eslint": "8.4.2",
    "@types/gradient-string": "^1.1.2",
    "@types/jscodeshift": "0.11.5",
    "@types/node": "^17.0.31",
    "@types/prettier": "2.6.0",
    "@types/prompts": "2.0.14",
    "@types/react": "18.0.8",
    "@types/rimraf": "3.0.2",
    "@types/tar": "6.1.1",
    "@types/validate-npm-package-name": "3.0.3",
    "@types/yargs": "^17.0.10",
    "c8": "7.11.2",
    "eslint": "8.14.0",
    "eslint-config-gpkt": "workspace:*",
    "esno": "^0.14.1",
    "jiti": "^1.13.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "unplugin": "0.6.2",
    "vitest": "0.10.4"
  }
}
