{
  "name": "gpkt",
  "version": "0.1.8",
  "description": "Create, build, and publish new packages with the gpkt scaffolding toolkit",
  "license": "MIT",
  "author": {
    "name": "Philip Johnston",
    "url": "https://github.com/tundera",
    "email": "tunderadev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/gpkt",
    "directory": "packages/gpkt"
  },
  "keywords": [
    "gpkt",
    "package",
    "generator"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "templates",
    "README.md"
  ],
  "bin": "./bin/gpkt.js",
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "esno src/cli.ts new my-cli --project-directory ../../../example-cli --preset cli",
    "lint": "TIMING=1 eslint --ext .ts,.tsx,.js,.jsx --cache --cache-location ./node_modules/.cache/eslint .",
    "prepublishOnly": "yarn build",
    "clean": "rimraf dist .eslintcache example"
  },
  "dependencies": {
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "async-retry": "1.3.3",
    "colorette": "2.0.16",
    "consola": "^2.15.3",
    "console-clear": "1.1.1",
    "convert-hrtime": "5.0.0",
    "cpy": "^9.0.1",
    "cross-spawn": "7.0.3",
    "ejs": "3.1.6",
    "execa": "6.1.0",
    "globby": "13.1.1",
    "got": "12.0.3",
    "ink": "3.2.0",
    "ink-big-text": "^1.2.0",
    "ink-gradient": "^2.0.0",
    "ink-spinner": "4.0.3",
    "ink-text-input": "4.0.3",
    "jscodeshift": "0.13.1",
    "merge-anything": "^5.0.2",
    "prompts": "2.4.2",
    "react": "18.0.0",
    "read-pkg": "^7.1.0",
    "rimraf": "^3.0.2",
    "tar": "6.1.11",
    "unconfig": "^0.3.2",
    "update-check": "1.5.4",
    "validate-npm-package-name": "4.0.0",
    "valtio": "1.5.2",
    "yaml": "2.0.0",
    "yargs": "17.4.1"
  },
  "devDependencies": {
    "@gpkt/eslint-config": "workspace:*",
    "@gpkt/tsconfig": "workspace:*",
    "@types/async-retry": "1.4.3",
    "@types/console-clear": "1.1.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "3.1.0",
    "@types/ink-big-text": "^1.2.1",
    "@types/ink-gradient": "^2.0.1",
    "@types/jscodeshift": "0.11.4",
    "@types/node": "^17.0.23",
    "@types/prettier": "2.6.0",
    "@types/prompts": "2.0.14",
    "@types/react": "18.0.1",
    "@types/rimraf": "3.0.2",
    "@types/tar": "6.1.1",
    "@types/validate-npm-package-name": "3.0.3",
    "@types/yargs": "^17.0.10",
    "bumpp": "^7.1.1",
    "eslint": "8.13.0",
    "esno": "^0.14.1",
    "jiti": "^1.13.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "unplugin": "0.6.1",
    "vitest": "0.9.3"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
