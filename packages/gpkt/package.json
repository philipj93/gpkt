{
  "name": "gpkt",
  "version": "0.1.1",
  "author": "Philip Johnston <tunderadev@gmail.com>",
  "description": "Create, build, and publish new packages with the gpkt scaffolding toolkit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/gpkt",
    "directory": "packages/gpkt"
  },
  "keywords": [
    "gpkt",
    "package",
    "generator"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "templates",
    "README.md"
  ],
  "bin": {
    "gpkt": "./bin/gpkt.js"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "esno src/cli.ts my-cli --project-directory ../../../super-cool-cli --template cli --use-pnpm",
    "lint": "TIMING=1 eslint --cache --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "TIMING=1 eslint --cache --ext .ts,.tsx,.js,.jsx . --fix",
    "prepublishOnly": "pnpm build",
    "clean": "rimraf dist .eslintcache"
  },
  "dependencies": {
    "async-retry": "1.3.3",
    "colorette": "2.0.16",
    "consola": "^2.15.3",
    "cpy": "^9.0.1",
    "cross-spawn": "7.0.3",
    "got": "12.0.2",
    "merge-anything": "^5.0.2",
    "prompts": "2.4.2",
    "read-pkg": "^7.1.0",
    "rimraf": "^3.0.2",
    "tar": "6.1.11",
    "unconfig": "^0.3.1",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@gpkt/eslint-config": "workspace:*",
    "@gpkt/tsconfig": "workspace:*",
    "@types/async-retry": "1.4.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^17.0.21",
    "@types/prompts": "2.0.14",
    "@types/rimraf": "3.0.2",
    "@types/tar": "6.1.1",
    "@types/validate-npm-package-name": "3.0.3",
    "@types/yargs": "^17.0.9",
    "bumpp": "^7.1.1",
    "eslint": "8.11.0",
    "esno": "^0.14.1",
    "jiti": "^1.13.0",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
