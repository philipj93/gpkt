{
  "version": "0.1.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "program": "./bin/program.js"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "esno src/cli.ts",
    "test": "vitest --run",
    "lint": "TIMING=1 eslint --ext .ts,.js --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist program",
    "release": "bumpp --commit --push --tag && yarn publish",
    "prepublishOnly": "yarn build",
    "prepare": "husky install",
    "commit": "gacp"
  },
  "dependencies": {
    "consola": "2.15.3",
    "dotenv": "16.0.0",
    "fs-extra": "10.0.1",
    "ohmyfetch": "0.4.15",
    "picocolors": "1.0.0",
    "unconfig": "0.3.4",
    "yargs": "17.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@types/eslint": "8.4.1",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.23",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "bumpp": "7.1.1",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "esno": "0.14.1",
    "gacp": "2.10.2",
    "husky": "7.0.4",
    "jiti": "1.13.0",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "tsconfig-paths": "3.14.1",
    "tsup": "5.12.4",
    "typescript": "4.6.3",
    "unbuild": "0.7.2"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "{{ packageExecutor }} lint --fix"
    ]
  },
  "gacp": {
    "emoji": "emoji"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
