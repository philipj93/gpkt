{
  "name": "@gpkt/transforms",
  "version": "0.1.8",
  "license": "MIT",
  "author": {
    "name": "Philip Johnston",
    "url": "https://github.com/tundera",
    "email": "tunderadev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/gpkt",
    "directory": "packages/transforms"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "TIMING=1 eslint --cache --ext .ts,.tsx,.js,.jsx .",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "yarn build",
    "clean": "rimraf dist .eslintcache"
  },
  "dependencies": {
    "@mrleebo/prisma-ast": "0.3.0",
    "@prisma/sdk": "3.12.0",
    "jscodeshift": "0.13.1",
    "recast": "0.20.5"
  },
  "devDependencies": {
    "@gpkt/eslint-config": "workspace:*",
    "@gpkt/tsconfig": "workspace:*",
    "@types/jest": "27.4.1",
    "@types/jscodeshift": "0.11.4",
    "@types/node": "17.0.23",
    "ast-types": "0.14.2",
    "bumpp": "7.1.1",
    "eslint": "8.13.0",
    "filedirname": "2.7.0",
    "jest": "27.5.1",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "tsup": "5.12.4",
    "typescript": "4.6.3",
    "vite": "2.9.1",
    "vitest": "0.9.3"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
